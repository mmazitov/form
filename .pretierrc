{
	"semi": true,
	"singleQuote": true,
	"jsxSingleQuote": false,
	"bracketSpacing": true,
	"jsxBracketSameLine": false,
	"trailingComma": "es5",
	"printWidth": 80,
	"tabWidth": 2,
	"arrowParens": "always",
	"useTabs": true,
	"endOfLine": "lf",
	"importOrder": [
		"^@/(.*)$",
		"^[./]"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
